{
  "name": "smart-house-backend",
  "version": "0.1.2",
  "description": "Smart House BE",
  "main": "dist/index.js",
  "private": false,
  "engines": {
    "node": "5.x.x",
    "npm": "3.3.x"
  },
  "scripts": {
    "start": "gulp serve",
    "build": "gulp",
    "lint": "gulp lint",
    "test": "cross-env ENV_CONFIG=test NODE_ENV=test mocha test/index.js",
    "test-watch": "cross-env ENV_CONFIG=test NODE_ENV=test mocha test/index.js -w",
    "test-coverage": "nyc npm test",
    "report-coverage": "codecov < ./coverage/lcov.info",
    "api": "cross-env ENV_CONFIG=production node src/runner.js",
    "postinstall": "babel src --out-dir dist"
  },
  "bin": "bin/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garage-it/SmartHouse-backend.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "mocha",
    "istanbul",
    "REST",
    "API",
    "smart",
    "house",
    "IoT"
  ],
  "dependencies": {
    "bluebird": "^3.1.4",
    "body-parser": "^1.14.2",
    "compression": "^1.6.1",
    "cookie-parser": "^1.4.1",
    "cors": "^2.7.1",
    "dotenv": "^2.0.0",
    "cross-env": "^3.1.3",
    "express": "~4.14.0",
    "express-validation": "^1.0.1",
    "express-winston": "^2.0.0",
    "http-status": "^0.2.0",
    "joi": "^9.2.0",
    "method-override": "^2.3.5",
    "mongoose": "^4.3.7",
    "morgan": "~1.7.0",
    "mqtt": "^2.0.1",
    "run-sequence": "^1.1.5",
    "rxjs": "^5.0.0-beta.6",
    "shelljs": "^0.7.0",
    "socket.io": "^1.4.5",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.4.1",
    "codecov": "^1.0.1",
    "debug": "^2.2.0",
    "del": "^2.2.0",
    "eslint": "^3.9.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.3.1",
    "eslint-plugin-standard": "^2.0.1",
    "glob": "^7.1.1",
    "gulp": "^3.9.0",
    "gulp-babel": "6.1.2",
    "gulp-env": "^0.4.0",
    "gulp-eslint": "^3.0.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-newer": "^1.1.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-plumber": "^1.0.1",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-util": "^3.0.7",
    "mocha": "^3.1.2",
    "nyc": "^8.3.2",
    "proxyquire": "^1.7.4",
    "sinon": "^1.17.3",
    "sinon-as-promised": "^4.0.0",
    "sinon-chai": "^2.8.0",
    "socket-io-mocks": "^0.1.4",
    "supertest": "^2.0.1",
    "supertest-as-promised": "^4.0.2"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/garage-it/SmartHouse-backend/issues"
  },
  "homepage": "https://github.com/garage-it/SmartHouse-backend#readme",
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "nyc": {
    "report-dir": "coverage",
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "exclude": [
      "node_modules",
      "**/*.test.js",
      "test/",
      "dist/",
      "coverage/",
      "bin/"
    ],
    "statements": 80,
    "branches": 80,
    "functions": 80,
    "lines": 80,
    "check-coverage": true
  }
}